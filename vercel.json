{
  "version": 2,
  "name": "swapkids",
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    },
    {
      "src": "landing.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/landing",
      "dest": "/landing.html"
    },
    {
      "src": "/api/(.*)",
      "dest": "api/index.py"
    },
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/landing.html"
    }
  ],
  "github": {
    "silent": true
  }
}
